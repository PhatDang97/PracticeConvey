{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "app": {
    "name": "convey some service",
    "displayBanner": true
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v2",
    "title": "Orders Service",
    "version": "v1",
    "routePrefix": "docs"
  },
  "webApi": {
    "bindRequestFromRoute": true
  },
  "security": {
    "certificate": {
      "enabled": true,
      "header": "Certificate",
      "allowedHosts": [ "localhost" ]
    }
  }
}
